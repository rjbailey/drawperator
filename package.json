  "bugs": {
    "url": "https://github.com/rjbailey/drawperator/issues"
  },
  "homepage": "https://github.com/rjbailey/drawperator#readme",
  "dependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.14.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "express": "4.14.0",
    "keymirror": "0.1.1",
    "lodash": "4.15.0",
    "node-uuid": "1.4.7",
    "randomstring": "1.1.5",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
